{
  "name": "@stain-win/qr-code-generator",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src/lib/qr-code-generator/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/qr-code-generator",
        "main": "./src/lib/qr-code-generator/index.ts",
        "tsConfig": "./tsconfig.lib.json",
        "assets": ["*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["./**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/qr-code-generator"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/qr-code-generator"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["src/lib/qr-code-generator/**/*.ts"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish"
      }
    }
  },
  "tags": []
}
